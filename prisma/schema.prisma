generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id            Int      @id @default(autoincrement())
  name          String
  imgUrl        String
  description   String
  price         Int
  created_at    DateTime
  modified_at   DateTime
  stripePriceId String?  @unique
}
